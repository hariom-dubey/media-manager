pipeline{
    agent any
    stages {
        stage('Setup Python Virtual ENV'){
            steps{
                sh '''
                chmod +x envsetup.sh
                ./envsetup.sh
                '''
            }
        }
        stage('Setup Gunicorn'){
            steps {
                sh '''
                chmod +x gunicorn.sh
                ./gunicorn.sh
                '''
            }
        }
        stage('Setup Nginx'){
            steps {
                sh '''
                chmod +x nginx_bash.sh
                ./nginx_bash.sh
                '''
            }
        }
    }
}